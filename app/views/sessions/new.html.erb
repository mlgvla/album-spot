<div class="container">

    <h1>Log In to The Album Spot</h1>
    
    <%= bootstrap_form_for :user, url:login_path do |f| %>
        <%= f.email_field :email %>
        <%= f.password_field :password %>
        <%= f.submit "Log In", class: "btn btn-primary" %>
    <% end %>

     <%= link_to "Log in with Github", "/auth/github" %>
 
</div>
